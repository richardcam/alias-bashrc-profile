# Custom prompt
PS1='\[\033[1;32m\]\u@\h \[\033[0;36m\]\w$(gbranch)\[\033[0m\]$ '

#Function to prompt
gbranch() {
git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/ (\1)/'
}

